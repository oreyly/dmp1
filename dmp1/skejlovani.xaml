<Window x:Class="dmp1.skejlovani"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:dmp1"
        xmlns:toolkit="http://schemas.xceed.com/wpf/xaml/toolkit"
        mc:Ignorable="d"
        Title="{Binding Path=(local:Uzivatel.ObrazekPozadi)}" 
        Height="{Binding Path=(local:HlavniStatik.Vyska), Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" 
        Width="{Binding Path=(local:HlavniStatik.Sirka), Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" 
        Left="{Binding Path=(local:HlavniStatik.Left), Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" 
        Top="{Binding Path=(local:HlavniStatik.Top), Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" 
        >
    <Grid>
        <Canvas x:Name="cnvGraf" SizeChanged="cnvGraf_SizeChanged"></Canvas>
        <Button Margin="126,95,110,132" Click="Button_Click">Ahoj</Button>
    </Grid>
</Window>
