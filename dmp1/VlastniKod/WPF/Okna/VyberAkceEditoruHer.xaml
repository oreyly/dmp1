<Window x:Class="dmp1.VyberAkceEditoruHer"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:dmp1"
        mc:Ignorable="d"
        Title="VyberAkceEditoruHer" Height="450" Width="800"
        Closed="Window_Closed"
        >
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="1*"></RowDefinition>
            <RowDefinition Height="1*"></RowDefinition>
            <RowDefinition Height="1*"></RowDefinition>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="1*"></ColumnDefinition>
            <ColumnDefinition Width="1*"></ColumnDefinition>
            <ColumnDefinition Width="1*"></ColumnDefinition>
        </Grid.ColumnDefinitions>

        <local:ProfilovyPrvek Grid.Column="2"></local:ProfilovyPrvek>
        <Button Grid.Row="0" Grid.Column="1" Click="Button_Click">Upravit existující úlohu</Button>
        <Button Grid.Row="1" Grid.Column="1" Click="Button_Click_1">Vytvořit vlastní úlohu</Button>
    </Grid>
</Window>
